QTabWidget::pane {
    border: 1px solid grey;
}
QTabBar::tab {
    color: rgb(167, 167, 167);
    background-color: qlineargradient(spread:reflect, x1:0, y1:1, x2:0, y2:0.409, stop:0 rgb(20, 20, 20), stop:1 rgb(20, 20, 20));
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border: 1px solid grey;
    border-bottom: none;
    padding: 5px;
    margin-left: 5px;
}
QTabBar::tab:selected {
    background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgb(7, 7, 7), stop:1 rgb(7, 7, 7));
    border: 1px solid grey;
    border-bottom:none;
    color: #4C7D24;
}
